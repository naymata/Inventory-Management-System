<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="207.0" prefWidth="288.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.inventory.inventory.controllers.MainController" fx:id="anchor">
    <VBox alignment="CENTER" fillWidth="false" prefHeight="159.0" prefWidth="289.0" spacing="20.0">
        <TextField fx:id="usernameField" alignment="CENTER" promptText="username"/>
        <PasswordField fx:id="passwordField" alignment="CENTER" promptText="password"/>
    </VBox>
    <Button fx:id="loginBtn" layoutX="14.0" layoutY="164.0" mnemonicParsing="false" onAction="#loginBtnAction"
            prefHeight="30.0" prefWidth="80.0" text="Login"/>
    <Button fx:id="closeBtn" layoutX="194.0" layoutY="164.0" mnemonicParsing="false" onAction="#closeBtnAction"
            prefHeight="30.0" prefWidth="80.0" text="Close"/>
</AnchorPane>
